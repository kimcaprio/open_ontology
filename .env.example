# Application Settings
APP_NAME=Ontology Platform
APP_VERSION=1.0.0
APP_ENV=development
LOG_LEVEL=INFO

# MinIO Settings
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_SECURE=false
MINIO_BUCKET_NAME=ontology-data

# Unity Catalog Settings
UC_ENDPOINT=http://localhost:8080/api/2.1/unity-catalog
UC_CATALOG_NAME=ontology_catalog

# Ollama Settings
OLLAMA_ENDPOINT=http://localhost:11434
OLLAMA_MODEL=llama3.1

# Search API Settings
EXA_API_KEY=your_exa_api_key
DUCKDUCKGO_REGION=us-en

# Database Settings (for metadata)
DATABASE_URL=sqlite:///./ontology.db

# API Settings
API_V1_PREFIX=/api/v1
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8000"] 